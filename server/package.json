    "dayjs": "1.11.10",
    "express": "5.0.0-beta.3",
    "express-beautify": "0.1.3",
    "helmet": "7.1.0",
    "intel": "1.2.0",
    "js-yaml": "4.1.0",
    "lodash.get": "4.4.2",
    "lodash.merge": "4.6.2",
