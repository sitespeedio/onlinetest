  "engineStrict": true,
  "keywords": [
    "sitespeed.io",
    "webperf",
    "webperformance"
  ],
  "license": "AGPL-3.0-only",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "app.js",
    "src",
    "views",
    "public",
    "package.json",
    "npm-shrinkwrap.json",
    "config",
    "db",
    "locales"
