{
  "name": "@sitespeed.io/server",
  "version": "0.0.3",
  "description": "The sitespeed.io server",
  "type": "module",
  "exports": "./app.js",
  "bin": {
    "sitespeed.io-server": "./app.js"
  },
  "engines": {
		"node": ">=20"
	},
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sitespeedio/onlinetest.git"
  },
  "bugs": {
    "url": "https://github.com/sitespeedio/onlinetest/issues"
  },
